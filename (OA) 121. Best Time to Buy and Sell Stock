class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int minPro = INT_MAX;
        int maxPro=0;
        int size=prices.size();
        for(int i=0;i<size;i++)
        {
            minPro=min(minPro,prices[i]);
            maxPro=max(maxPro,prices[i]-minPro);
        }
        return maxPro;
    }
};
